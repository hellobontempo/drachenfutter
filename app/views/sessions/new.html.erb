<div class="center">
<h3>Log In:</h3>

<%= form_for :user do |f|%>
    <%= f.label :email %><br>
    <%= f.email_field :email %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <%= f.submit "Log In"%><br>
<% end %>


<%= button_to "Log in with Google", '/auth/google_oauth2'  %>
</div>